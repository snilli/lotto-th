		"drizzle-orm": "^0.31.2",
		"drizzle-zod": "^0.5.1",
		"nest-commander": "^3.13.0",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1",
		"zod": "^3.23.8"
	},
