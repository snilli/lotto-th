		"@nestjs/common": "^10.3.9",
		"@nestjs/config": "^3.2.2",
		"@nestjs/core": "^10.3.9",
		"@nestjs/platform-express": "^10.3.9",
		"@nestjs/swagger": "^8.0.1",
		"@vercel/postgres": "^0.8.0",
		"agentkeepalive": "^4.5.0",
		"axios": "^1.7.2",
		"cheerio": "1.0.0-rc.12",
		"compression": "^1.7.4",
